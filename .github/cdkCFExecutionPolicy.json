{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "apigateway:*",
        "cloudwatch:*",
        "cognito-identity:*",
        "cognito-idp:*",
        "cognito-sync:*",
        "dynamodb:*",
        "kms:*",
        "lambda:*",
        "logs:*",
        "s3:*",
        "ssm:*",
        "secretsmanager:*",
        "cloudtrail:*",
        "events:*",
        "servicecatalog:*",
        "cloudformation:DescribeStacks",
        "glue:Create*",
        "glue:Delete*",
        "firehose:Create*",
        "firehose:Delete*",
        "firehose:DescribeDeliveryStream",
        "athena:Create*",
        "athena:Delete*",
        "athena:GetWorkGroup",
        "sqs:CreateQueue",
        "sqs:DeleteQueue",
        "sqs:GetQueueAttributes",
        "sqs:SetQueueAttributes"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "iam:*Role*",
        "iam:GetPolicy",
        "iam:CreatePolicy",
        "iam:DeletePolicy",
        "iam:*PolicyVersion*"
      ],
      "NotResource": [
        "arn:aws:iam::*:role/cdk-*",
        "arn:aws:iam::*:policy/cdkCFExecutionPolicy"
      ]
    }
  ]
}
