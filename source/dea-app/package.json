{
  "name": "@aws/dea-app",
  "version": "0.0.1",
  "description": "This package sets up API routes for DEA",
  "homepage": "https://code.amazon.com/packages/Digital-evidence-archive/trees/mainline",
  "repository": {
    "type": "git",
    "url": "https://code.amazon.com/packages/Digital-evidence-archive/trees/mainlinet"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "build": "heft build",
    "build:clean": "heft build --clean",
    "build:test": "heft test && rushx pkg-json-lint && rushx make-badges",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "create-cognito-user": "npx ts-node src/test/local-dev-support/create-user.ts",
    "delete-cognito-user": "npx ts-node src/test/local-dev-support/delete-user.ts",
    "depcheck": "depcheck",
    "e2e:only": ". ./setEnv.sh && heft test --no-build --test-path-pattern '.*/*e2e.test.*'",
    "get-user-creds": "npx ts-node src/test/local-dev-support/get-user-creds.ts",
    "integration:only": "heft test --no-build  --test-path-pattern '.*/*integration.test.*'",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "serve": "node lib/dev-launch.js",
    "sort-package-json": "sort-package-json package.json",
    "start": "heft start",
    "test": "rushx test:only && rushx make-badges",
    "test:only": ". ./setEnv.sh && heft test --no-build",
    "unit:only": "heft test --no-build  --test-path-pattern '.*/*unit.test.*'"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.264.0",
    "@aws-sdk/client-cognito-identity-provider": "3.264.0",
    "@aws-sdk/client-dynamodb": "3.264.0",
    "@aws-sdk/client-s3": "3.264.0",
    "@aws-sdk/client-ssm": "3.264.0",
    "@aws-sdk/s3-request-presigner": "3.264.0",
    "@types/aws-lambda": "^8.10.110",
    "@types/crypto-js": "^4.1.1",
    "aws-jwt-verify": "^3.4.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk-client-mock": "^2.0.1",
    "aws-sdk-client-mock-jest": "^2.0.1",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21",
    "puppeteer": "18.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@casl/ability": "~6.3.3",
    "@rushstack/eslint-config": "^3.0.0",
    "@rushstack/heft": "^0.48.8",
    "@rushstack/heft-jest-plugin": "^0.3.28",
    "@rushstack/heft-node-rig": "^1.10.5",
    "@types/cors": "^2.8.12",
    "@types/heft-jest": "1.0.2",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.181",
    "@types/minimist": "~1.2.2",
    "@types/node": "^14",
    "@types/node-fetch": "2.6.2",
    "@types/uuid": "^8.3.4",
    "aws-cdk": "~2.63.0",
    "aws-cdk-lib": "~2.63.0",
    "aws4-axios": "~2.4.9",
    "axios": "^0.27.1",
    "concurrently": "^7.0.0",
    "constructs": "10.0.5",
    "depcheck": "^1.4.3",
    "dynamo-db-local": "~5.0.0",
    "dynamodb-onetable": "~2.5.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-security": "^1.4.0",
    "istanbul-badges-readme": "1.8.1",
    "jest": "^27.5.1",
    "joi": "~17.7.0",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "minimist": "~1.2.8",
    "node-fetch": "^3.3.0",
    "nodemon": "^2.0.19",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^27.1.3",
    "ts-mockito": "~2.6.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "wait-port": "~1.0.4"
  }
}
