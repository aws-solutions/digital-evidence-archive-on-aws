{
  "name": "@aws/dea-app",
  "version": "1.0.5",
  "description": "This package sets up API routes for DEA",
  "homepage": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/tree/main/",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/tree/main/"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "acl:only": ". ../common/scripts/setEnv.sh && heft test --no-build --disable-code-coverage --test-path-pattern '.*/*e2e.acl.test.*'",
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "audit:only": ". ../common/scripts/setEnv.sh && heft test --no-build --disable-code-coverage --test-path-pattern '.*/*e2e.audit.test.*'",
    "build": "heft build",
    "build:clean": "heft build --clean",
    "build:test": ". ../common/scripts/setEnv.sh && heft test && rushx pkg-json-lint && rushx make-badges",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "create-cognito-user": ". ../common/scripts/setEnv.sh && npx ts-node src/test/local-dev-support/create-user.ts",
    "delete-cognito-user": ". ../common/scripts/setEnv.sh && npx ts-node src/test/local-dev-support/delete-user.ts",
    "e2e:only": ". ../common/scripts/setEnv.sh && heft test --no-build --disable-code-coverage --test-path-pattern '.*/*e2e.test.*'",
    "get-user-creds": ". ../common/scripts/setEnv.sh && npx ts-node src/test/local-dev-support/get-user-creds.ts",
    "idp-test-setup": ". ../common/scripts/setEnv.sh && npx ts-node src/test/local-dev-support/idp-test-setup.ts",
    "integration:only": ". ../common/scripts/setEnv.sh && heft test --no-build --disable-code-coverage  --test-path-pattern '.*/*integration.test.*'",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "serve": "node lib/dev-launch.js",
    "sort-package-json": "sort-package-json package.json",
    "start": "heft start",
    "test": "rushx test:only && rushx make-badges",
    "test:dev": ". ../common/scripts/setEnv.sh && heft test --watch",
    "test:noe2e": ". ../common/scripts/setEnv.sh && heft test --no-build --test-path-pattern '.*/*(integration|unit).test.*' && rushx make-badges",
    "test:only": ". ../common/scripts/setEnv.sh && heft test --no-build",
    "test:sonar": "rushx unit:only",
    "unit:only": ". ../common/scripts/setEnvDummy.sh && heft test --no-build --test-path-pattern '.*/*unit.test.*'",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "~3.398.0",
    "@aws-sdk/client-cloudwatch-logs": "~3.398.0",
    "@aws-sdk/client-cognito-identity": "~3.398.0",
    "@aws-sdk/client-cognito-identity-provider": "~3.398.0",
    "@aws-sdk/client-dynamodb": "~3.398.0",
    "@aws-sdk/client-firehose": "~3.398.0",
    "@aws-sdk/client-s3": "~3.398.0",
    "@aws-sdk/client-s3-control": "~3.398.0",
    "@aws-sdk/client-secrets-manager": "~3.398.0",
    "@aws-sdk/client-ssm": "~3.398.0",
    "@aws-sdk/client-sts": "~3.398.0",
    "@aws-sdk/s3-request-presigner": "~3.398.0",
    "@aws-sdk/types": "~3.398.0",
    "@aws/workbench-core-audit": "~0.1.1",
    "@casl/ability": "~6.3.3",
    "@types/aws-lambda": "^8.10.114",
    "@types/crypto-js": "^4.1.1",
    "@types/jsonwebtoken": "~9.0.1",
    "aws-cdk": "2.88.0",
    "aws-cdk-lib": "2.88.0",
    "aws-jwt-verify": "^4.0.0",
    "aws-lambda": "^1.0.7",
    "constructs": "10.2.69",
    "crypto-js": "^4.1.1",
    "csv-string": "~4.1.1",
    "dynamodb-onetable": "2.5.1",
    "joi": "~17.9.1",
    "jsonwebtoken": "~9.0.0",
    "lodash": "^4.17.21",
    "pkce-challenge": "^3.0.0",
    "ulid": "~2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "^3.3.3",
    "@rushstack/heft": "^0.50.0",
    "@rushstack/heft-jest-plugin": "^0.5.5",
    "@rushstack/heft-node-rig": "^1.12.5",
    "@types/cors": "^2.8.13",
    "@types/heft-jest": "1.0.3",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/minimist": "~1.2.2",
    "@types/node": "^18.15.7",
    "@types/node-fetch": "2.6.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.60.0",
    "@typescript-eslint/parser": "~5.60.0",
    "aws-sdk-client-mock": "^2.1.1",
    "aws-sdk-client-mock-jest": "^2.1.1",
    "aws4-axios": "~2.4.9",
    "axios": "^0.27.1",
    "concurrently": "^7.6.0",
    "dynamo-db-local": "~5.0.1",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "istanbul-badges-readme": "1.8.5",
    "jest": "^29.5.0",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "minimist": "~1.2.8",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "puppeteer": "19.8.0",
    "sort-package-json": "^2.4.1",
    "ts-mockito": "~2.6.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "wait-port": "~1.0.4"
  }
}
