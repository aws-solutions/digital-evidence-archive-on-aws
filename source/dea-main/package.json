{
  "name": "@aws/dea-main",
  "version": "1.1.1",
  "description": "Main stack for deploying DEA",
  "homepage": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/tree/main/",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/tree/main/"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "bin": {
    "infrastructure": "lib/index.js"
  },
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "audit-redrive": ". ../common/scripts/setRedriveEnv.sh && npx ts-node ../dea-app/src/app/migrations/audit-redrive-command.ts",
    "audit:only": "",
    "build": "heft build",
    "build:clean": "heft build --clean",
    "build:test": "heft test --verbose --test-path-pattern test && rushx make-badges",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy": "../common/scripts/preDeploy.sh && cdk deploy --all --require-approval never --outputs-file cdk-outputs.json",
    "cdk:destroy": "../common/scripts/preDestroy.sh && cdk destroy --force",
    "cdk:synth": "cdk synth",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "e2e:only": "",
    "integration:only": "heft test --no-build --disable-code-coverage --test-path-pattern '.*/*integration.test.*'",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "mdi:only": "",
    "nag": "cfn_nag_scan --fail-on-warnings --input-path ./cdk.out/DeaMainStack.template.json",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "sort-package-json": "sort-package-json package.json",
    "test": "rushx test:only && rushx make-badges",
    "test:dev": "heft test --test-path-pattern test --watch",
    "test:noe2e": ". ../common/scripts/setEnv.sh && heft test --no-build --test-path-pattern '.*/*(integration|unit).test.*' && rushx make-badges",
    "test:noe2enocdk": "heft test --no-build --test-path-pattern '.*/*.(integration|unit).test.*' && rushx make-badges",
    "test:only": "heft test --no-build --test-path-pattern test",
    "cdkunit:only": ". ../common/scripts/setEnv.sh && heft test --no-build --verbose --test-path-pattern '.*/*cdkunit.test.*'",
    "cdkunit:only:u": ". ../common/scripts/setEnv.sh && heft test -u --no-build --verbose --test-path-pattern '.*/*cdkunit.test.*'",
    "test:sonar": "rushx test:only",
    "unit:only": ". ../common/scripts/setEnvDummy.sh && heft test --no-build --test-path-pattern '.*/*.unit.test.*'",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "validate:config": "npx ts-node ../dea-backend/src/config.ts",
    "generate:config": "npx ts-node ../dea-backend/src/deployment-helpers/generate-config-command.ts"
  },
  "dependencies": {
    "@aws-cdk/aws-glue-alpha": "~2.86.0-alpha.0",
    "@aws/dea-backend": "workspace:*",
    "@aws/dea-ui": "workspace:*",
    "@aws/dea-ui-infrastructure": "workspace:*",
    "aws-cdk": "2.133.0",
    "aws-cdk-lib": "2.133.0",
    "aws-lambda": "^1.0.7",
    "constructs": "10.2.69",
    "dynamodb-onetable": "2.5.1",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "^3.3.3",
    "@rushstack/heft": "^0.50.0",
    "@rushstack/heft-jest-plugin": "^0.5.5",
    "@rushstack/heft-node-rig": "^1.12.5",
    "@types/heft-jest": "1.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.7",
    "@typescript-eslint/eslint-plugin": "~5.60.0",
    "@typescript-eslint/parser": "~5.60.0",
    "axios": "^1.6.5",
    "esbuild": "^0.17.13",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "istanbul-badges-readme": "1.8.5",
    "jest": "^29.5.0",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sort-package-json": "^2.4.1",
    "ts-mockito": "~2.6.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
