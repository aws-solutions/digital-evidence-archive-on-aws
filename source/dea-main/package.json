{
  "name": "@aws/dea-main",
  "version": "0.0.1",
  "description": "Example infrastructure to deploy express app",
  "homepage": "https://code.amazon.com/packages/Digital-evidence-archive/trees/mainline",
  "repository": {
    "type": "git",
    "url": "https://code.amazon.com/packages/Digital-evidence-archive/trees/mainline"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "bin": {
    "infrastructure": "lib/index.js"
  },
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "build": "heft build",
    "build:clean": "heft build --clean",
    "build:test": "heft test --verbose --test-path-pattern test && rushx make-badges",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy": "cdk deploy --all --require-approval never --outputs-file cdk-outputs.json",
    "cdk:synth": "cdk synth",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "depcheck": "depcheck",
    "e2e:only": "heft test --no-build --test-path-pattern '.*/*e2e.test.*'",
    "integration:only": "heft test --no-build  --test-path-pattern '.*/*integration.test.*'",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "nag": "cfn_nag_scan --fail-on-warnings --input-path ./cdk.out/DeaMainStack.template.json",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "sort-package-json": "sort-package-json package.json",
    "test": "rushx test:only && rushx make-badges",
    "test:integration": "heft test --no-build",
    "test:only": "heft test --no-build --test-path-pattern test",
    "test:noe2e": "heft test --no-build --test-path-pattern '.*/*(integration|unit).test.*' && rushx make-badges",
    "test:only:u": "heft test -u --no-build --verbose --test-path-pattern test",
    "unit:only": "heft test --no-build  --test-path-pattern '.*/*unit.test.*'"
  },
  "dependencies": {
    "@aws/dea-backend": "workspace:*",
    "@aws/dea-ui": "workspace:*",
    "@aws/dea-ui-infrastructure": "workspace:*",
    "aws-lambda": "^1.0.7",
    "constructs": "10.0.5",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "^3.0.0",
    "@rushstack/heft": "^0.48.8",
    "@rushstack/heft-jest-plugin": "^0.3.28",
    "@rushstack/heft-node-rig": "^1.10.5",
    "@types/heft-jest": "1.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.181",
    "@types/node": "^14",
    "aws-cdk": "~2.63.0",
    "aws-cdk-lib": "~2.63.0",
    "axios": "^0.27.1",
    "constructs": "10.0.5",
    "depcheck": "^1.4.3",
    "esbuild": "^0.14.21",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-security": "^1.4.0",
    "istanbul-badges-readme": "1.8.1",
    "jest": "^27.5.1",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sort-package-json": "^1.57.0",
    "ts-mockito": "~2.6.1",
    "typescript": "^4.5.2"
  }
}
