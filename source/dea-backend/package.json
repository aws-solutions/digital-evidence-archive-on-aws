{
  "name": "@aws/dea-backend",
  "version": "1.0.5",
  "description": "Example infrastructure to deploy express app",
  "homepage": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/tree/main/",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/tree/main/"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/index.js",
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "audit:only": "",
    "build": "heft build",
    "build:clean": "heft build --clean",
    "build:test": ". ../common/scripts/setEnv.sh && heft test --verbose --test-path-pattern test && rushx make-badges",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy": "cdk deploy --all --require-approval never --outputs-file cdk-outputs.json",
    "cdk:synth": "cdk synth",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "e2e:only": ". ../common/scripts/setEnv.sh && heft test --no-build --disable-code-coverage --test-path-pattern '.*/*e2e.test.*'",
    "integration:only": "heft test --no-build --disable-code-coverage --test-path-pattern '.*/*integration.test.*'",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "sort-package-json": "sort-package-json package.json",
    "test": "rushx test:only && rushx make-badges",
    "test:dev": ". ../common/scripts/setEnv.sh && heft test --test-path-pattern test --watch",
    "test:noe2e": ". ../common/scripts/setEnv.sh && heft test --no-build --test-path-pattern '.*/*(integration|unit).test.*' && rushx make-badges",
    "test:only": ". ../common/scripts/setEnv.sh && heft test --no-build --verbose --test-path-pattern test",
    "test:only:u": ". ../common/scripts/setEnv.sh && heft test -u --no-build --verbose --test-path-pattern test",
    "test:sonar": "rushx unit:only",
    "unit:only": ". ../common/scripts/setEnvDummy.sh && heft test --no-build --test-path-pattern '.*/*unit.test.*'",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.22.0-alpha.0",
    "@aws-cdk/aws-glue-alpha": "~2.86.0-alpha.0",
    "@aws-sdk/client-cloudformation": "~3.398.0",
    "@aws-sdk/client-kinesis": "~3.398.0",
    "@aws-sdk/client-s3": "~3.398.0",
    "@aws/dea-app": "workspace:*",
    "@types/aws-lambda": "^8.10.114",
    "aws-cdk": "2.88.0",
    "aws-cdk-lib": "2.88.0",
    "aws-lambda": "^1.0.7",
    "constructs": "10.2.69",
    "convict": "~6.2.4",
    "dynamodb-onetable": "2.5.1",
    "joi": "~17.9.1",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.68.0",
    "@aws-cdk/aws-kinesisfirehose-alpha": "~2.88.0-alpha.0",
    "@aws-cdk/aws-kinesisfirehose-destinations-alpha": "~2.88.0-alpha.0",
    "@aws-cdk/aws-servicecatalogappregistry-alpha": "~2.78.0-alpha.0",
    "@aws-sdk/client-cloudwatch-logs": "~3.398.0",
    "@rushstack/eslint-config": "^3.3.3",
    "@rushstack/heft": "^0.50.0",
    "@rushstack/heft-jest-plugin": "^0.5.5",
    "@rushstack/heft-node-rig": "^1.12.5",
    "@types/convict": "~6.1.1",
    "@types/heft-jest": "1.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.7",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.60.0",
    "@typescript-eslint/parser": "~5.60.0",
    "axios": "^0.27.1",
    "esbuild": "^0.17.13",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "istanbul-badges-readme": "1.8.5",
    "jest": "^29.5.0",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sort-package-json": "^2.4.1",
    "ts-mockito": "~2.6.1",
    "typescript": "^4.5.2"
  }
}
