{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.90.0",
  "pnpmVersion": "7.16.0",
  "nodeSupportedVersionRange": ">=18.12.0 <19.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMaxDepth": 5,
  "approvedPackagesPolicy": {
    "reviewCategories": ["production", "tools", "prototypes"],
    "ignoredNpmScopes": ["@types"]
  },
  "gitPolicy": {
    "versionBumpCommitMessage": "Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "Update changelogs [skip ci]"
  },
  "repository": {
    "url": "git@github.com:aws-solutions/digital-evidence-archive-on-aws.git",
    "defaultBranch": "mainline"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "@aws/dea-app",
      "projectFolder": "dea-app",
      "reviewCategory": "production",
      "shouldPublish": false
    },
    {
      "packageName": "@aws/dea-main",
      "projectFolder": "dea-main",
      "reviewCategory": "production",
      "shouldPublish": false
    },
    {
      "packageName": "@aws/dea-backend",
      "projectFolder": "dea-backend",
      "reviewCategory": "production",
      "shouldPublish": false
    },
    {
      "packageName": "@aws/dea-ui",
      "projectFolder": "dea-ui/ui",
      "reviewCategory": "production",
      "shouldPublish": false
    },
    {
      "packageName": "@aws/dea-ui-infrastructure",
      "projectFolder": "dea-ui/infrastructure",
      "reviewCategory": "production",
      "shouldPublish": false
    }
  ]
}
