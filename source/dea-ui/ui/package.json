{
  "name": "@aws/dea-ui",
  "version": "0.0.1",
  "description": "NextJS frontend UI for Digital Evidence Archive",
  "homepage": "https://code.amazon.com/packages/Digital-evidence-archive/trees/dea-ui",
  "repository": {
    "type": "git",
    "url": "https://code.amazon.com/packages/Digital-evidence-archive/blobs/dea-ui/--/README.md"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "build": "next build && next export --silent",
    "build-only": "next build",
    "build:test": "rushx build && rushx test",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "depcheck": "depcheck",
    "deploy-ui": "bash ./scripts/deploy.sh",
    "dev": "next dev",
    "export": "next build && next export",
    "lint": "next lint && rushx pkg-json-lint",
    "lint:fix": "next lint --fix",
    "pkg-json-lint": "npmPkgJsonLint -c ../../../.npmpackagejsonlintrc.json .",
    "serve": "node .next/server/webpack-runtime.js",
    "sort-package-json": "sort-package-json package.json",
    "test": "jest --coverage --passWithNoTests",
    "test:only": "jest --ci --coverage --passWithNoTests"
  },
  "dependencies": {
    "@cloudscape-design/components": "^3.0.27",
