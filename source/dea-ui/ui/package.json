{
  "name": "@aws/dea-ui",
  "version": "1.1.1",
  "description": "NextJS frontend UI for Digital Evidence Archive",
  "homepage": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/blob/main/source/dea-ui/ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-solutions/digital-evidence-archive-on-aws/blob/main/source/dea-ui/ui/README.md"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "audit:only": "",
    "build": "./setNextEnv.sh && next build",
    "build-only": "./setNextEnv.sh && next build",
    "build:test": "rushx build && rushx test && rushx make-badges",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "cypress": "cypress open",
    "cypress:e2e": ". ../../common/scripts/setEnv.sh && cypress run --config baseUrl=${DEA_API_URL}ui",
    "cypress:headless": "cypress run",
    "cypress:test": "start-server-and-test start http-get://localhost:3000/${STAGE}/ui cypress:headless",
    "dev": ". ../../common/scripts/setEnv.sh && ./setNextEnv.sh && next dev",
    "dev:https": "(local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000 &) && rushx dev",
    "e2e:only": "",
    "integration:only": "jest --ci --testPathPattern '.*/*integration.test.*'",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "next lint --max-warnings=0 && rushx pkg-json-lint",
    "lint:fix": "next lint --fix --max-warnings=0",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "pkg-json-lint": "npmPkgJsonLint -c ../../../.npmpackagejsonlintrc.json .",
    "mdi:only": "",
    "serve": "node .next/server/webpack-runtime.js",
    "sort-package-json": "sort-package-json package.json",
    "start": "next start",
    "test": "jest --coverage && rushx make-badges",
    "test:dev": "jest --watch",
    "test:noe2e": "jest --ci --coverage '.*/*(integration|unit).test.*' && rushx make-badges",
    "test:only": "jest --ci --coverage",
    "cdkunit:only": "echo ':)'",
    "cdkunit:only:u": "echo ':)'",
    "test:sonar": "rushx test:only",
    "unit:only": "jest --ci '.*/*unit.test.*'",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "~3.474.0",
    "@aws-sdk/client-cognito-identity": "~3.474.0",
    "@aws-sdk/client-cognito-identity-provider": "~3.474.0",
    "@aws-sdk/client-s3": "~3.474.0",
    "@aws/dea-app": "workspace:*",
    "@cloudscape-design/collection-hooks": "^1.0.41",
    "@cloudscape-design/component-toolkit": "~1.0.0-beta.25",
    "@cloudscape-design/components": "~3.0.588",
    "@cloudscape-design/design-tokens": "~3.0.34",
    "@cloudscape-design/global-styles": "~1.0.25",
    "@cloudscape-design/jest-preset": "~2.0.32",
    "@types/crypto-js": "^4.1.1",
    "aws4-axios": "~3.3.0",
    "axios": "^1.7.2",
    "crypto-js": "~4.2.0",
    "date-fns": "^2.28.0",
    "i18next": "~21.10.0",
    "jest-environment-jsdom": "~29.6.3",
    "jsonwebtoken": "~9.0.0",
    "jwt-decode": "^3.1.2",
    "next": "^14.2.3",
    "next-global-css": "^1.2.0",
    "next-i18next": "^11.0.0",
    "pkce-challenge": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "~11.18.6",
    "swr": "^1.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.23.2",
    "@babel/preset-env": "~7.23.2",
    "@babel/preset-react": "~7.22.5",
    "@babel/preset-typescript": "~7.23.2",
    "@rushstack/eslint-config": "^3.3.3",
    "@rushstack/heft": "^0.50.0",
    "@rushstack/heft-jest-plugin": "^0.5.5",
    "@rushstack/heft-node-rig": "^1.12.5",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "6.3.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/heft-jest": "1.0.3",
    "@types/jest": "^29.5.0",
    "@types/lodash.kebabcase": "~4.1.7",
    "@types/node": "^18.15.7",
    "@types/react": "17.0.38",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.60.0",
    "@typescript-eslint/parser": "~5.60.0",
    "aws-cdk": "2.133.0",
    "aws-cdk-lib": "2.133.0",
    "babel-jest": "27.5.1",
    "concurrently": "^7.6.0",
    "constructs": "10.2.69",
    "cypress": "~13.1.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-testing-library": "5.10.2",
    "istanbul-badges-readme": "1.8.5",
    "jest": "^29.5.0",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "local-ssl-proxy": "~2.0.5",
    "lodash.kebabcase": "~4.1.1",
    "nodemon": "^2.0.22",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sass": "^1.60.0",
    "sort-package-json": "^2.4.1",
    "start-server-and-test": "2.0.3",
    "ts-mockito": "~2.6.1",
    "typescript": "^4.5.2"
  }
}
